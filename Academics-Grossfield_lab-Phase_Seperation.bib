@article{Feigenson2007,
abstract = {Bilayer mixtures of lipids are used by many researchers as chemically simple models for biological membranes. In particular, observations on three-component bilayer mixtures containing cholesterol show rich phase behavior, including several regions of two-phase coexistence and one region of three-phase coexistence. Yet, the relationship between these simple model mixtures and biological membranes, which contain hundreds of different proteins and lipids, is not clear. Many of the model mixtures have been chosen for study because they exhibit readily observed phase separations, not because they are good mimics of cell membrane components. If the many components of cell membranes could be grouped in some way, then understanding the phase behaviors of biological membranes might be enhanced. Furthermore, if the underlying interaction energies between lipids and proteins can be determined, then it might be possible to model the distributions of lipids and proteins in a bilayer membrane, even in complex mixtures. Copyright {\textcopyright} 2007 by Annual Reviews. All rights reserved.},
author = {Feigenson, Gerald W.},
doi = {10.1146/annurev.biophys.36.040306.132721},
file = {:C$\backslash$:/Users/ashli/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Feigenson - 2007 - Phase boundaries and biological membranes.pdf:pdf},
issn = {10568700},
journal = {Annu. Rev. Biophys. Biomol. Struct.},
keywords = {Multicomponent mixture,Phase diagram,Ternary mixture,Thermodynamics of mixing},
pages = {63--77},
pmid = {17201675},
title = {{Phase boundaries and biological membranes}},
volume = {36},
year = {2007}
