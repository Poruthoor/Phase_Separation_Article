file = {:C$\backslash$:/Users/ashli/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Elson et al. - 2010 - Phase separation in biological membranes Integration of theory and experiment.pdf:pdf},
issn = {1936122X},
journal = {Annu. Rev. Biophys.},
keywords = {Continuum theory,Line tension,Lipid nanodomains,Membrane rafts,Monte Carlo simulation,Nanoscopic detection},
number = {1},
pages = {207--226},
pmid = {20192775},
title = {{Phase separation in biological membranes: Integration of theory and experiment}},
volume = {39},
year = {2010}
}
